{
    "compilerOptions": {
      "types": ["react", "react-native", "jest"],
      "target": "es2015",
      "module": "es2015",
      "moduleResolution": "node",
      "jsx": "react-native",
      "outDir": "build",
      "rootDir": "src"
    },
    "include": ["src/**/*"],
    "exclude": [
      "android",
      "ios",
      "compiled",
      "node_modules"
    ]
}